<?xml version="1.0" encoding="utf-8"?>
<com.nex3z.flowlayout.FlowLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="265dp"
    android:layout_height="wrap_content"
    app:flChildSpacing="0dp"
    app:flChildSpacingForLastRow="align"
    app:flRowSpacing="10dp"
    tools:context=".Main">

    <ImageButton
        android:id="@+id/papers_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/papers"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/glass_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/glass"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/plastic_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/plastic"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/metal_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/metal"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/cloths_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/cloths"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/other_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/other"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/dangerous_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/dangerous"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/batteries_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/batteries"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/lamp_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/lamp"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/appliances_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/appliances"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/tetra_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/tetra"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/lid_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/lid"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/tires_menu_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/tires"
        android:scaleType="fitCenter" />

</com.nex3z.flowlayout.FlowLayout>