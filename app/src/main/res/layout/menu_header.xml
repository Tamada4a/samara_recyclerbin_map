<?xml version="1.0" encoding="utf-8"?>
<com.nex3z.flowlayout.FlowLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="265dp"
    android:layout_height="wrap_content"
    app:flChildSpacing="0dp"
    app:flChildSpacingForLastRow="align"
    app:flRowSpacing="10dp"
    tools:context=".Main"
    tools:ignore="MissingClass">



    <ImageView
        android:layout_width="215dp"
        android:layout_height="59dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/side_menu_label" />

    <ImageButton
        android:id="@+id/papers_menu_button"
        android:layout_width="55dp"
        android:layout_height="66dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/papers"
        android:scaleType="fitCenter"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/glass_menu_button"
        android:layout_width="55dp"
        android:layout_height="66dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/glass"
        android:scaleType="fitCenter"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/plastic_menu_button"
        android:layout_width="55dp"
        android:layout_height="66dp"
